{
  "name": "B-API",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:api": "ts-node src/test-api.ts",
    "test:api-client": "jest src/tests/api-client.test.ts",
    "test:api-performance": "jest src/tests/api-performance.test.ts",
    "test:api-integration": "jest src/tests/api-integration.test.ts",
    "test:api-all": "jest src/tests/",
    "test:mcp": "jest src/tests/routes/mcpAnalytics.test.ts",
    "test:comprehensive": "jest src/tests/services/comprehensiveAnalytics.integration.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "start:mcp": "cd mcp-backend-server && npm run start-all",
    "ai:setup": "npm install && cd mcp-backend-server && npm install && npm run build",
    "ai:start": "concurrently \"npm run dev\" \"npm run mcp:start-all\"",
    "ai:analyze": "cd mcp-backend-server && node test-servers.js",
    "ai:test": "npm test && cd mcp-backend-server && npm test",
    "ai:full-setup": "npm run ai:setup && npm run db:setup && npm run ai:analyze",
    "mcp:build": "cd mcp-backend-server && npm run build",
    "mcp:start-all": "cd mcp-backend-server && npm run build && echo 'All MCP servers ready for GitHub Copilot!'",
    "mcp:test": "cd mcp-backend-server && node test-servers.js",
    "db:setup": "docker-compose -f docker-compose.dev.yml up -d postgres redis",
    "db:stop": "docker-compose -f docker-compose.dev.yml down",
    "db:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "db:clean": "docker-compose -f docker-compose.dev.yml down -v"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
